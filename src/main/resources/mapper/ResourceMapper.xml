<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sjxm.springbootinit.mapper.MyResourceMapper">

    <resultMap id="BaseResultMap" type="com.sjxm.springbootinit.model.entity.MyResource">
            <id property="id" column="id" jdbcType="BIGINT"/>
            <id property="name" column="name" jdbcType="VARCHAR"/>
            <result property="resources" column="resources" jdbcType="VARCHAR"/>
            <result property="subjectId" column="subject_id" jdbcType="BIGINT"/>
            <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
            <result property="createUser" column="create_user" jdbcType="VARCHAR"/>
            <result property="updateTime" column="update_time" jdbcType="TIMESTAMP"/>
            <result property="updateUser" column="update_user" jdbcType="VARCHAR"/>
            <result property="deleteFlag" column="delete_flag" jdbcType="TINYINT"/>
            <result property="brief" column="brief" jdbcType="VARCHAR"/>
            <result property="size" column="size" jdbcType="DECIMAL"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,name,resources,
        subject_id,create_time,create_user,
        update_time,update_user,delete_flag,
        brief,size
    </sql>
</mapper>
